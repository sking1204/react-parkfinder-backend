{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "review_title",
        "review_data",
        "rating"
    ],
    "properties": {
        "review_title": {
            "title": "The review_title Schema",
            "type": "string",
            "default": "",
            "examples": [
                "Yellowstone Review"
            ]
        },
        "review_data": {
            "title": "The review_data Schema",
            "type": "string",
            "default": "",
            "examples": [
                "This is my review"
            ]
        },
        "rating": {
            "title": "The rating Schema",
            "type": "integer",
            "default": 0,
            "examples": [
                1
            ]
        }
    },
    "examples": [{
        "review_title": "Yellowstone Review",
        "review_data": "This is my review",
        "rating": 1
    }]
}