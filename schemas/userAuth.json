{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "username",
        "password"
    ],
    "properties": {
        "username": {
            "title": "The username Schema",
            "type": "string",
            "default": "",
            "examples": [
                "testuser"
            ]
        },
        "password": {
            "title": "The password Schema",
            "type": "string",
            "default": "",
            "examples": [
                "password"
            ]
        }
    },
    "examples": [{
        "username": "testuser",
        "password": "password"
    }]
}