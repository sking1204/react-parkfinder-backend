{
    "$schema": "https://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "username",
        "password",
        "firstName",
        "lastName",
        "email"
        
    ],
    "properties": {
        "username": {
            "title": "The username Schema",
            "type": "string",
            "default": "",
            "minLength": 1,
            "maxLength": 30,
            "examples": [
                "Test1"
            ]
        },
        "password": {
            "title": "The password Schema",
            "type": "string",
            "default": "",
            "minLength": 5,
            "maxLength": 20,
            "examples": [
                "Password"
            ]
        },
        "firstName": {
            "title": "The firstName Schema",
            "type": "string",
            "default": "",
            "minLength": 1,
            "maxLength": 30,
            "examples": [
                "Test"
            ]
        },
        "lastName": {
            "title": "The lastName Schema",
            "type": "string",
            "default": "",
            "minLength": 1,
            "maxLength": 30,
            "examples": [
                "1"
            ]
        },
        "email": {
            "title": "The email Schema",
            "type": "string",
            "default": "",
            "minLength": 6,
            "maxLength": 60,
            "format": "email",
            "examples": [
                "test1@email.com"
            ]
        },
        "isAdmin": {
            "title": "The isAdmin Schema",
            "type": "boolean",
            "default": false,
            "examples": [
                true
            ]
        }
    },
    "examples": [{
        "username": "Test1",
        "password": "Password",
        "firstName": "Test",
        "lastName": "1",
        "email": "test1@email.com",
        "isAdmin": true
    }]
}